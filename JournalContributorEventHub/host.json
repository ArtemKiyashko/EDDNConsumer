{
  "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingExcludedTypes": "Request",
      "samplingSettings": {
        "isEnabled": true
      }
    },
    "logLevel": {
      "default": "Information",
      "Host": "Error",
      "Function": "Error",
      "Host.Aggregator": "Information"
    },
    "extensions": {
      "eventHubs": {
        "batchCheckpointFrequency": 1,
        "eventProcessorOptions": {
          "maxBatchSize": 1024,
          "prefetchCount": 2048
        }
      }
    }
  }
}